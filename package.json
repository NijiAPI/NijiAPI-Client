  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "typescript": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NijiAPI/nijiapi-client.git"
  },
  "bugs": {
    "url": "https://github.com/NijiAPI/nijiapi-client/issues"
